module PropElim where 

-- Define natural numbers in Set
data Nat : Set :=
  Zero : Nat
  Succ : Nat -> Nat

-- Define the Even predicate on Nat in Prop with multiple constructors
data Even : Nat -> Prop :=
  EvenZero : Even Zero
  EvenSuccSucc : Even n -> Even (Succ (Succ n))

data Set1 : Set :=
  elt1 : Set1
  elt2 : Set1

invalidElimEvenToSet : Even n -> Set
invalidElimEvenToSet = \ev . case ev of
  EvenZero -> Set1       -- Should fail
  EvenSuccSucc n' -> Set1 -- Should fail