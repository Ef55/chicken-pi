module Bool where

--{ Features: datatypes }
data Bool : Type :=
  True : Bool
  False : Bool

--{ Features: matching }
and : Bool -> Bool -> Bool
and = \l r . case l of
  True -> r
  False -> False

--{ Features: matching:return }
or : Bool -> Bool -> Bool
or = \ l r . case l of
  True -> True
  False -> r